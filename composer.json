{
    "name": "rowbot/dom",
    "description": "A modern alternative to PHP's built in DOM classes.",
    "authors": [
        {
            "name": "Trevor Rowbotham",
            "homepage": "http://trowbotham.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "rowbot/url": "3.1.4"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.93",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
        "slevomat/coding-standard": "^7.0.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Rowbot\\DOM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rowbot\\DOM\\Tests\\": "tests/"
        }
    }
}
